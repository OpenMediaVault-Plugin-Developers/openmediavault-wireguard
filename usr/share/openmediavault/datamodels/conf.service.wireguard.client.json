{
  "type": "config",
  "id": "conf.service.wireguard.client",
  "title": "wireguard client",
  "queryinfo": {
    "xpath": "//services/wireguard/clients/client",
    "iterable": true,
    "idproperty": "uuid"
  },
  "properties": {
    "uuid": {
      "type": "string",
      "format": "uuidv4"
    },
    "enable": {
      "type": "boolean",
      "default": false
    },
    "clientnum": {
      "type": "integer",
      "minimum": 1,
      "maximum": 253
    },
    "tunnelnum": {
      "type": "integer",
      "minimum": 1,
      "maximum": 255
    },
    "clientname": {
      "type": "string"
    },
    "restrict": {
      "type": "boolean"
    },
    "localip": {
      "type": "boolean"
    },
    "persistent": {
      "type": "integer",
      "minimum": 0,
      "maximum": 3600
    },
    "privatekeyclient": {
      "type": "string"
    },
    "publickeyclient": {
      "type": "string"
    },
    "presharedkeyclient": {
      "type": "string"
    },
    "dns": {
      "type": "string"
    }
  }
}
