{
  "type": "config",
  "id": "conf.service.wireguard.tunnel",
  "title": "wireguard tunnel",
  "queryinfo": {
    "xpath": "//services/wireguard/tunnels/tunnel",
    "iterable": true,
    "idproperty": "uuid"
  },
  "properties": {
    "uuid": {
      "type": "string",
      "format": "uuidv4"
    },
    "enable": {
      "type": "boolean",
      "default": false
    },
    "tunnelnum": {
      "type": "integer",
      "minimum": 1,
      "maximum": 255
    },
    "tunnelname": {
      "type": "string"
    },
    "nic": {
      "type": "string"
    },
    "endpoint": {
      "type": "string"
    },
    "localserver": {
      "type": "string"
    },
    "port": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535,
      "default": 51820
    },
    "iptables": {
      "type": "boolean",
      "default": true
    },
    "persistent": {
      "type": "integer",
      "minimum": 0,
      "maximum": 3600
    },
    "mtu": {
      "type": "integer",
      "minimum": 0,
      "maximum": 9999
    },
    "privatekeyserver": {
      "type": "string"
    },
    "publickeyserver": {
      "type": "string"
    }
  }
}
